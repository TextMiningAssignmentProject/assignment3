54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591 \uadc0\uc5ec\uc6b4 \ub108\uad6c\ub9ac \uac19\uc740 \uc138\uc774\ube14 \ud3ec\uba54\ub77c\ub2c8\uc548 \ub315\ub315\uc774!\uc548\ub155\ud558\uc138\uc694 \uc787\ub2d8\ub4e4!!!!!\u200b\u200b\uc624\ub298\ub3c4 \ud589\ubcf5\ud55c \ud558\ub8e8 \ubcf4\ub0b4\uc2dc\uace0\uacc4\uc2dc\ub098\uc6a9 ???\uc800\ub294 \uc9c0\uae08 \uc5c4\uccad \ud589\ubcf5\ud558\ub2f5\ub2c8\ub2f9!\uc65c\ub0d0\ud558\uba74!! \uc6b0\ub9ac \uadc0\uc5ec\uc6b4\ub108\uad6c\ub9ac \uac19\uc740 \ud3ec\uba54\ub77c\ub2c8\uc548 \uc544\uac00\ubcf4\ub2c8 \ud790\ub9c1 \ub418\ub294 \ub9c8\uc74c\uc5d0 \ubaa8\ub4e0\uac8c\ub2e4 \ub179\uc544 \ub0b4\ub9ac\ub294,,,,\ub118 \uadc0\uc5fd\uc9c0 \uc54a\ub098\uc6a9 ?\u314b\u314b\u314b\uc800\ub294 \ud3ec\uba54\uac00 \uc138\uc0c1\uc5d0\uc11c\uc81c\uc77c \uc88b\uc544\uc6a9!!!!!!\ud83d\udc97\ub118 \uadc0\uc5fd\uc796\uc544\uc694!!!!\ud83d\udc97\uc138\uc0c1\uc5d0 \ub9c8\uc0c1\uc5d0\ub098\uc19c\uc0ac\ud0d5 \uac19\uc544\uc694 \u3160\u3160\u3160\ub2ec\ub2ec\ud55c \uadc0\uc5fc\ub465\uc774 ! \u314b\u314b\u314b\u200b\uc6b0\ub9ac \uc560\uae30 \ubc14\ub85c \ubcf4\uc2dc\uc981 !!!\uc138\uc774\ube14\ud3ec\uba54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591\u200b14~16\uc138\uae30\uc758 \ub974\ub124\uc0c1\uc2a4 \uc2dc\ub300\uc5d0\ub3c4\ubc18\ub824\uacac\uc73c\ub85c \ub9ce\uc740\uc0ac\ub791\uc744 \ubc1b\uc558\ub2e4\uace0 \ud574\uc694\ub974\ub124\uc0c1\uc2a4\uc758 \uac70\uc7a5\uc774\ub77c\uace0\ubd88\ub9ac\ub294 \ud654\uac00\uc778 \ubbf8\ucf08\ub780\uc824\ub85c\ub3c4\ud3ec\uba54\ub77c\ub2c8\uc548\uc744 \ubc18\ub824\uacac\uc73c\ub85c \ub450\uc5c8\uace0,\ub85c\ub9c8\uc758 \uad50\ud669\ub4e4\ub3c4\ud3ec\uba54\ub77c\ub2c8\uc548\uc744 \uae38\ub800\ub2e4\uace0 \ud574\uc6a9.\u200b\uc544\uc774\uc2ac\ub780\ub4dc\uc640 \ub7a9\ub79c\ub4dc \uc9c0\uc5ed\uc5d0\uc11c\uc370\ub9e4\ub97c \ub04c\ub358 \uacac\uc885\uc778\uc800\uba3c \uc2a4\ud53c\uce20\uc640 \uc0ac\ubaa8\uc608\ub4dc\ub97c \uc18c\ud615\ud654\uc2dc\ud0a8 \uc885\uc73c\ub85c \uc774\ub984\uc740\ub3c5\uc77c \ud3ec\uba54\ub77c\ub2c8\uc544 \uc9c0\uc5ed\uc5d0\uc11c \uc720\ub798\uac00 \ub418\uc5c8\ub2f5\ub2c8\ub2f9!\u2764\u200b1768\ub144 \uc0ec\ub7ff \uc655\ube44\uc5d0 \uc758\ud574\uc601\uad6d\uc5d0\uc11c \ucc98\uc74c \uc18c\uac1c\ub41c\ud3ec\uba54\ub77c\ub2c8\uc548\uc740\ub9c8\ub9ac\uc559\ud2b8\uc640\ub124\ud2b8\uc640 \ubaa8\ucc28\ub974\ud2b8\uac00\ud0a4\uc6b0\uae30\ub3c4 \ud588\ub358 \uacac\uc885\uc73c\ub85c!\uc601\uad6d\uc5d0 \uc54c\ub824\uc9c4 \ub4a4\uc5d0\ube45\ud1a0\ub9ac\uc544 \uc5ec\uc655\uc5d0\uac8c\uae4c\uc9c0 \uc0ac\ub791\uc744 \ubc1b\uc544\uc11c\uc655\uc2e4\uacac\uc774\ub77c\ub294 \ud0c0\uc774\ud2c0\ub3c4 \uc5bb\uac8c \ub418\uc5c8\ub2e4\uace0 \ud574\uc6a9\uc61b\ub0a0 \ud3ec\uba54\ub77c\ub2c8\uc548\uc740\uc370\ub9e4\ub97c \ub04c\uae30\uc704\ud574\ud0a4\uc6e0\uae30 \ub54c\ubb38\uc5d0 \uc9c0\uae08\uc758\ubaa8\uc2b5\ubcf4\ub2e4 \ud6e8\uc52c \ud06c\uace0 \ud48d\ubd80\ud55c \ud138\uc744\uac00\uc9c0\uace0 \uc788\uc5c8\uc9c0\ub9cc\uc9c0\uae08\uc740 \ud488\uc885\uc744 \uac1c\ub7c9\uc2dc\ucf1c\uc9c0\uae08\uc758 \uc791\uc740 \ud3ec\uba54\ub77c\ub2c8\uc548\uc774\ud0c4\uc0dd\ud588\uc2b5\ub2c8\ub2f9\u270c\uc138\uc774\ube14\ud3ec\uba54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591\u200b\ud3ec\uba54\ub77c\ub2c8\uc548\uc740 \uc791\uace0 \uc559\uc99d\ub9de\uc740 \uccb4\uad6c\uc5d0\uadc0\uc5ec\uc6b4 \uc678\ubaa8\ub97c \uac00\uc9c0\uace0 \uc788\ub294\ub370\uc5b4\ub9b4 \ub54c\ub294 \uc544\uae30\uacf0\uac19\uae30\ub3c4 \ud558\uace0\uc19c\ubb49\uce58\uac19\uae30\ub3c4 \ud55c\ub370 \uc131\uacac\uc774 \ub418\uba74\uc791\uc740 \uc5ec\uc6b0 \uac19\uae30\ub3c4\ud558\ub294 \ub4f1\ub2e4\uc591\ud55c \ub9e4\ub825\uc744 \ubf50\ub0b4\uc6a9\ud138\uc774 \uacf5\uac19\uc774 \ub465\uae00\uac8c \ubd80\ud47c \ud138\uc740\ud3ec\uba54\ub77c\ub2c8\uc548\uc758 \uccb4\uad6c\ub97c \uc870\uae08 \ub354\ucee4\ubcf4\uc774\uac8c \ud574\uc694\ud3ec\uba54\ub77c\ub2c8\uc548\uc758 \ud138\ub85c \uacf0\ub3cc\uc774 \ucef7\ubbf8\uc6a9\uc744\uc2dc\ud0a4\uba74 \uc815\ub9d0 \uc2ec\uc7a5\uc5d0\ubb34\ub9ac\uac00 \uc62c \uc218\ub3c4\uc788\uc5b4\uc694 ...!!\uc815\ub9d0 \ub108\ubb34 \uadc0\uc5fd\uac70\ub4e0\uc694\u314b\u314b\u314b\u314b\ud3ec\uba54\ub77c\ub2c8\uc548\uc740\uc774\uc911\ubaa8 \uacac\uc885\uc774\uae30 \ub54c\ubb38\uc5d0\ud138\ube60\uc9d0\uc774 \ub9ce\uc740 \ud3b8\uc774\ub77c \uadc0\uc5ec\uc6b4\uc678\ubaa8\uc640 \ud138 \uc0c9\uc744 \ubcf4\uace0 \ub370\ub824\uc654\ub2e4\uac04\uc54c\ub808\ub974\uae30, \uccad\uc18c \ub4f1\uc758\ubb38\uc81c\ub85c \uace0\ubbfc\uc5d0 \ube60\uc9c8 \uc218 \uc788\uc5b4\uc694!\ud83e\udd29\uc138\uc774\ube14\ud3ec\uba54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591\u200b\ud3ec\uba54\ub77c\ub2c8\uc548 \uc758 \ud06c\uae30\ub29422~28cm, \ubab8\ubb34\uac8c\ub2941.8~3kg \uc815\ub3c4\uc758 \uc18c\ud615\uacac\uc5d0\uc18d\ud558\ub294 \uc560\uae30\uc778\ub370\uc694??\uc704\uc5d0 \uc0ac\uc9c4\uc5d0 \uc788\ub294\ud06c\ub9bc\ud3ec\uba54\ub77c\ub2c8\uc548 \uc544\uac00\ub294\uc131\uacac\uc77c \ub54c \ubab8\ubb34\uac8c1kg~2kg \uc608\uc0c1\uc911\uc774\uc5d0\uc694!\uc5b4\ub9b0\ub370\ub3c4 \ud3ec\ub3c4\uc824\ub9ac \uaf49\ucc44\uc6cc\uc9c0\uace0 \uc788\uc5b4\uc11c \uc131\uacac\uc77c\ub54c\ub3c4 \ub108\ubb34\ub098\ub3c4 \uc791\uc744\uac70\uac19\uc740\ubbf8\ub2c8\ud3ec\uba54\ub77c\ub2c8\uc548\ub97c \uc5f0\uc0c1\uc2dc\ud0a4\uc8e0\ud83d\udc9b\u200b\u200b\uc9c1\ubaa8\uc778 \uc774\uc911\ubaa8\uc774\uae30 \ub54c\ubb38\uc5d0\ud138\uc774 \ubab8\uc5d0 \ubd99\uc9c0 \uc54a\uace0\ubd95 \ub5a0\uc788\ub294 \uc19c\uc0ac\ud0d5, \uc19c\ubb49\uce58\uac19\uc740 \uc678\ud615\uc774 \uc5c4\uccad\ub098\uac8c \ub9e4\ub825\uc801\uc73c\ub85c \ub290\uaef4\uc9c0\uae30\ub3c4 \ud558\uc8e0 \u314b\u314b\u314b\u2726\u203f\u2726\ud3ec\uba54\ub77c\ub2c8\uc548 \uc740\ubaa9\uc695\uc744 \ud560\ub54c\ub098 \ud138\uc744\uc544\uc8fc \uc9e7\uac8c \uc790\ub974\ub294 \uacbd\uc6b0\uc5d0\ub294\uce58\uc640\uc640 \uac19\uc740 \uc774\ubbf8\uc9c0\ub3c4\uc8fc\ub294\ub370\uc694??? \u0669( \u141b )\ud3ec\uba54\ub77c\ub2c8\uc548 \uc758 \uc19c\uc0ac\ud0d5\uac19\uc740\ud138\uc5d0 \ubb3c\uc774 \ub2ff\uc544\uc11c \ucd94\uc6b1\uac00\ub77c\uc549\uac8c\ub418\uba74\uc644\uc804 \ubb3c\uc816\uc740 \uce58\uc640\uc640\uac19\uc544\uc694 \u314b\u314b\u314b\u314b\uadf8\ub798\uc11c \ub354\uc6b1 \ub2e4\uc591\ud55c\ubaa8\uc2b5\uacfc \ub9e4\ub825\uc744 \ubf50\ub0b4\uae30\uc5d0\ud3ec\uba54\ub77c\ub2c8\uc548 \uc744 \ub354\uc6b1\uc804\uc138\uacc4 \uc0ac\ub78c\ub4e4\uc5d0\uac8c\uc0ac\ub791\uc744 \ubc1b\ub294 \uac83\uc774 \uc544\ub2d0\uae4c\uc2f6\uc5b4\uc694!!(\u270c\uff9f\u2200\uff9f)\u261e\uc138\uc774\ube14\ud3ec\uba54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591\u200b\u200b\ud06c\ub9bc, \uc624\ub80c\uc9c0, \uc138\uc774\ube14, \ube14\ub799\ud0c4,\ud30c\ud2f0, \ube14\ub799\ub4f1 \ub9ce\uc740 \ubaa8\uc0c9\uc774 \uc788\uc5b4\uc694 .\ud2b9\ud788 \ud30c\ud2f0\ud3ec\uba54\ub294 \ube14\ub799-\ud654\uc774\ud2b8 ,\ud68c\uac08\uc0c9-\ud654\uc774\ud2b8, \ube0c\ub77c\uc6b4-\ud654\uc774\ud2b8\uc774\ub807\uac8c \uc5ec\ub7ec\uac00\uc9c0 \uceec\ub7ec\ub97c\uac00\uc9c0\uace0 \uc788\ub2f5\ub2c8\ub2e4.\uc9c0\uae08 \uc6b0\ub9ac \uc787\ub2d8\ub4e4\uc774 \ubcf4\uace0\uacc4\uc2e0\ud3ec\uba54\ub77c\ub2c8\uc548 \uc560\uae30\ub294 \ud06c\ub9bc\ud3ec\uba54\ub77c\ub2c8\uc548 \uc774\ub78d\ub2c8\ub2e4 \u314e\u314e\ubcf4\uace0\uacc4\uc2e0 \uc0ac\uc9c4 \uc18d \ud3ec\uba54\ub77c\ub2c8\uc548\uc560\uae30\ub294 ?? \u270c(\u2018\u03c9\u2019)\u270c\uc5c4\uccad\ub098\uac8c \ucabc\uaf2c\ubbf8\ud55c \uccb4\uad6c\uc5d0\uc624\ubc00\uc870\ubc00 \uc5bc\uad74\uc5d0 \ub208\ucf54\uc785 \ub2e4\ubaa8\uc5ec\uc788\ub294 \ubc14\ub451\ub3cc\uac19\uc740 \ub208 \ucf54!!\ub2e4\ub978 \ud3ec\uba54\ub77c\ub2c8\uc548 \uc560\uae30\ub4e4\ubcf4\ub2e4\ub354\uc6b1 \ub354 \ubaa8\ub7c9\uc774 \ube75\ube75\ud55c\ud3ec\uba54\ub77c\ub2c8\uc548 \ub315\ub315\uc774\uc774\ub78d\ub2c8\ub2e4 \u314e\u314e\uc138\uc774\ube14\ud3ec\uba54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591\u200b\u200b\uc0dd\ud6c4 3~4\uac1c\uc6d4 \ucbe4\ubd80\ud130\ud798\uc5c6\uace0 \ud478\uc11d\ud55c\ubc30\ub0c7\ud138\uc774 \ube60\uc9c0\uae30 \uc2dc\uc791\ud558\uace0\ud2bc\ud2bc\ud55c \uc601\uad6c\ud138\uc774 \uc790\ub77c\ub294\ud138\uac08\uc774\ub97c \uc2dc\uc791\ud558\uac8c \ub418\ub294\ub370\uc774 \uc2dc\uae30\uc5d0\ub294 \uc804\uccb4\uc801\uc73c\ub85c\ud138\uc774 \ube60\uc9c0\uac8c\ub418\uba74\uc11c \uc5bc\uad74\ubd80\ubd84\uc774\uc6d0\uc22d\uc774\uc640 \ub2ee\uc558\ub2e4\uace0 \ud574\uc11c\uc6d0\uc22d\uc774 \uc2dc\uae30\ub77c\uace0\ubd80\ub974\ub294 \uac70\ub78d\ub2c8\ub2f9 (>\u032f-\u032e<\u032f)\uc774 \uc2dc\uae30\uc5d0\ub294 \ub9ce\uc740 \uc591\uc758\ud138\uc774 \ube60\uc9c0\uac8c \ub418\ub294\ub370\uc774 \ud604\uc0c1\uc740 \uc790\uc5f0\uc2a4\ub7ec\uc6b4\ud604\uc0c1\uc774\ub77c\uc11c \ub2e4\uc2dc \ud2bc\ud2bc\ud558\uace0\uc724\uae30\ub098\ub294\ud138\uc774 \ub098\uac8c \ub418\ubbc0\ub85c\uac71\uc815\ud558\uc9c0 \uc54a\uc73c\uc154\ub3c4 \ub41c\ub2f5\ub2c8\ub2f9\ube57\uc9c8\uc744 \uc790\uc8fc \ud574\uc8fc\uae30\ub9cc \ud558\uba74\uc6d0\uc22d\uc774 \uc2dc\uae30\uc5d0\ub3c4\uc6c0\uc774 \ub9ce\uc774 \ub418\uc694.!!\ud3ec\uba54\ub77c\ub2c8\uc548\uc740 \uc120\ucc9c\uc801\uc73c\ub85c\ubf08\uc640 \uad00\uc808\uc774\uc57d\ud558\uac8c \ud0dc\uc5b4\ub098\uc11c\uc2ac\uac1c\uace8 \ud0c8\uad6c\uc758 \uc704\ud5d8\uc131\uc774\uc81c\uc77c \ud070 \uc885\uc778\ub370\uc5b4\ub824\uc11c\ubd80\ud130 \ubbf8\ub044\ub7ec\uc6b4 \ubc14\ub2e5\uc5d0\uc11c\ub6f0\uac70\ub098 \ub192\uc740 \uacf3\uc5d0\uc11c\ub6f0\uc5b4\ub0b4\ub9ac\ub294 \ud589\ub3d9\uc744 \uc870\uc2ec\ud574\uc57c\ud558\uace0\ub450\uaebc\uc6b4 \uc774\uc911\ubaa8\uc774\uae30 \ub54c\ubb38\uc5d0\ud53c\ubd80\uac00 \uac74\uc870\ud574\uc9c8 \ud655\ub960\uc774\ub9e4\uc6b0 \ub192\uc544\uc11c \ud53c\ubd80 \ud2b8\ub7ec\ube14\uc774\ub098\uae30 \uc26c\uc6cc\uc11c \uc8fc\uae30\uc801\uc778\ubaa9\uc695\uacfc \ud53c\ubd80\uad00\ub9ac\ub294 \ud544\uc218! ( . \u032b .)\ud83d\udc97\uc138\uc774\ube14\ud3ec\uba54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591\u200b\u200b\uae30\uac04 \ud5c8\ud0c8 \uc774\ub77c\ub294\ubcd1\uc774 \uc788\ub294\ub370\uc694?\uc18c\ud615\uacac\uc774\ub098 \ud37c\uadf8 \ub4f1\uc758\ub2e8\ub450\uc885\uc5d0 \ub9ce\uc740 \uc9c8\ud658\uc774\uae30\uad00\ud5c8\ud0c8\uc774\uc5d0\uc694! \u0295\u2022\u0361\u0c6a\u2022\u0294\u200b\u200b\ud3ec\uba54\ub77c\ub2c8\uc548 \uac19\uc740 \uc791\uc740\uc18c\ud615\uacac\ub4e4\uc5d0\uac8c\ud638\ubc1c\ud558\ub294 \uad00\uc808\uc9c8\ud658\uc73c\ub85c\ub294\uc2ac\uac1c\uace8\ud0c8\uad6c\uac00 \uc788\uc5b4\uc694!\uc5b4\ub9b4 \ub54c\ubd80\ud130 \uac78\uc74c\uac78\uc774\ub97c\uc720\uc2ec\ud788 \uad00\ucc30\ud558\uace0 \uc774\ubcc0\uc744\uc54c\uc544\ucc28\ub9ac\uba74 \ubcd1\uc6d0\uc5d0\uc11c\uc815\ud655\ud55c \uc9c4\ub2e8\uc744 \ubc1b\uc544\uc57c\ud569\ub2c8\ub2e4!! \u02a2\u1d55\u1d17\u1d55\u02a1\u200b\u200b\uc0ac\ub78c\uc774 \uba39\ub294 \uc74c\uc2dd\uc740\ub418\ub3c4\ub85d \uc8fc\uc9c0 \uc54a\ub294 \uac83\uc774\uac00\uc7a5 \uc88b\uace0 \uc18c\ud654\uc5d0\uc88b\uc740 \ubc18\ub824\uacac \uc804\uc6a9\uc0ac\ub8cc\ub098 \uc74c\uc2dd\uc744 \uc8fc\ub294 \uac83\uc774 \uc88b\uaca0\uc8e0?\ubaa8\ub4e0 \uac15\uc544\uc9c0\ub4e4\uc774\uc0ac\ub78c\uc74c\uc2dd\uc744 \uba39\uc73c\uba74\uc548\ub418\ub294 \ub9cc\ud07c\ud3ec\uba54\ub294 \ub354\ub354\uc6b1 \uc8fc\uba74 \uc548\ub41c\ub2e4\ub294 \uc810! \u02d9\u2c19\u02d9\uc138\uc774\ube14\ud3ec\uba54\ub77c\ub2c8\uc548\ucc45\uc784\ubd84\uc591\u200b\uc9c8\ud22c\uc2ec\ub3c4 \uc0c1\ub2f9\ud558\ub2f5\ub2c8\ub2f9 \u25df\u0295\u00b4\u2200`\u0294\u25de\uac81\ub3c4 \ub9ce\uc9c0\ub9cc \uc790\uae30\ubcf4\ub2e4 \ubab8\uc9d1\uc774\ud070 \uac15\uc544\uc9c0\ud55c\ud14c\ub3c4 \uac81\ub0b4\uc9c0 \uc54a\uace0\uc9d6\ub294 \uc559\uce7c\uc9c4 \uba74\ub3c4\uac00\uc9c0\uace0 \uc788\uc5b4\uc6a9 \u314b\u314b\u314b\u314b\ubaa9\uc695\uc744 \uc2dc\ud0a4\uba74 \uce58\uc640\uc640\ub791\ub3c4\ub2ee\uc558\ub2e4\ub294 \uc0dd\uac01\uc774 \ub4e4\uc815\ub3c4\ub85c\ud3ec\uba54\ub77c\ub2c8\uc548\uc740 \ud138 \ube68\uc774\ub78d\ub2c8\ub2f9 \u314b\u314b\uc774\ub7f0\uac8c \ub9e4\ub825\uc544\ub2d0\uae4c\uc6a9 ! \u0295\u2661\u02d9\u1d25\u02d9\u2661\u0294\uc790\uae30\uc8fc\uc7a5\ub3c4 \uac15\ud558\uace0 \uace0\uc9d1\uc774\uc5c4\uccad \uc318 \uc544\uc774\uc5d0\uc6a9 \u314b\u314b\uc8fc\uc778\uc5d0 \ub300\ud55c \uc9